  <h1>投稿一覧画面</h1>
  <% @users.each do |user| %>
    <p>お名前：<%= user.name %></p>
    <p>年齢：<%= user.age %></p>
    <%= link_to "削除" , "/users/#{user.id}", method: :delete, data: {confirm: "本当に削除しますか?"} %>
    <%= link_to "編集", "/users/#{user.id}/edit", method: :get %>
  <% end %>

  <%= link_to "投稿画面へ", "/users/new" %>
